{
  "name": "montage",
  "version": "0.13.0-alpha",
  "publishConfig": {
    "tag": "edge"
  },
  "description": "A UI Framework",
  "main": "montage",
  "bin": {
    "montage": "bin/montage"
  },
  "overlay": {
    "browser": {
      "main": "core/core",
      "mappings": {
        "mr": {
          "name": "mr",
          "location": "packages/mr"
        },
        "q": {
          "name": "q",
          "location": "packages/mr/packages/q"
        }
      },
      "exclude": [
        "lab",
        "etc",
        "node.js"
      ]
    }
  },
  "dependencies": {
    "collections": "~0.1.17",
    "frb": "~0.1.26",
    "mousse": "~0.1.0"
  },
  "devDependencies": {
    "montage-testing": "~0.1.3",
    "mr": "~0.12.6",
    "q": "~0.8.12",
    "q-io": "~1.3.2",
    "jsdom": "~0.2.19"
  }
}
