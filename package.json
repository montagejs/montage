{
    "name": "montage",
    "version": "0.10.0",
    "description": "A UI Framework",
    "main": "montage",
    "overlay": {
        "browser": {
            "main": "core/core",
            "exclude": [
                "lab/benchmarks",
                "lab/sandbox",
                "samples",
                "tools",
                "examples",
                "require/test",
                "require/tests",
                "etc",
                "node.js"
            ],
            "resources": [
                "montage.js",
                "node_modules/mr/require.js",
                "node_modules/mr/browser.js",
                "node_modules/q/q.js"
            ]
        }
    },
    "bin": {
        "montage": "bin/montage"
    },
    "mappings": {
        "mr": {
            "location": "packages/mr"
        },
        "q": {
            "location": "package/q"
        }
    },
    "engines": {
        "node": ">=0.6.0 <0.6.11"
    }
}
