{
    "name": "montage",
    "version": "0.12.1",
    "description": "A UI Framework",
    "main": "montage",
    "bin": {
        "montage": "bin/montage"
    },
    "overlay": {
        "browser": {
            "main": "core/core",
            "exclude": [
                "lab",
                "etc",
                "node.js"
            ]
        }
    },
    "mappings": {
        "mr": {
            "name": "mr",
            "location": "packages/mr"
        },
        "q": {
            "name": "q",
            "location": "packages/mr/packages/q"
        }
    }
}
