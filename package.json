{
  "name": "montage",
  "version": "0.13.4",
  "description": "A UI Framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/montagejs/montage.git"
  },
  "main": "core/core",
  "browser": {
      "url": "mr/mini-url.js"
  },
  "dependencies": {
    "collections": "~0.1.23",
    "frb": "~0.2.10",
    "mousse": "~0.1.2",
    "htmlparser2": "~3.0.5"
  },
  "devDependencies": {
    "montage-testing": "~0.2.0",
    "mr": "~0.13.0",
    "q": "0.9.6",
    "q-io": "~1.7.2",
    "jshint": "~2.0.1"
  },
  "scripts": {
    "build": "npm run build-montage && npm run build-preload",
    "build-montage": "mrs boot/browser.js -e '().done()'  > montage.js",
    "build-preload": "mrs boot/preload.js > boot/preload-boilerplate.js",
    "test": "node test/node/node-spec.js",
    "jsdoc": "node ../montagejs.org/build/jsdoc/jsdoc.js -out $PWD/builds/jsdoc/ montage@npm-link"
  },
  "exclude": [
    "etc",
    "lab",
    "node.js",
    "boot",
    "test",
    "tools"
  ]
}
