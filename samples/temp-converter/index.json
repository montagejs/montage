{
    "owner": {
        "prototype": "temp-converter[Main]"
    },

    "converter": {
        "prototype": "temp-converter[TempConverter]"
    },

    "celsiusTextfield": {
        "prototype": "montage/ui/input-text.reel",
        "properties": {
            "element": {"#": "celsius"}
        },
        "bindings": {
            "value": {"<->": "@owner.value", "converter": {"@": "converter"}}
        }
    },

    "fahrenheitTextfield": {
        "prototype": "montage/ui/input-text.reel",
        "properties": {
            "element": {"#": "fahrenheit"},
            "value": "32"
        },
        "bindings": {
            "value": {"<->": "@owner.value"}
        }
    },

    "barometer": {
        "prototype": "montage/ui/input-range.reel",
        "properties": {
            "orientation": "vertical",
            "element": {"#": "barometer"}
        },
        "bindings": {
            "value": {"<->": "@owner.value"}
        }
    }
}
