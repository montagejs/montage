{
    "button_description_autofocus":{
        "prototype":"ui/component-description[ComponentPropertyDescription]",
        "properties":{
            "_componentDescription":{
                "@":"root"
            },
            "_name":"autofocus",
            "_valueType":"string",
            "helpString":"Specifies that a button should automatically get focus when the page loads"
        }
    },

    "button_description_enabled":{
        "prototype":"ui/component-description[ComponentPropertyDescription]",
        "properties":{
            "_componentDescription":{
                "@":"root"
            },
            "_name":"enabled",
            "_valueType":"boolean",
            "helpString":"Specifies that a button should be enabled"
        }
    },

    "button_description_form":{
        "prototype":"ui/component-description[ComponentPropertyDescription]",
        "properties":{
            "_componentDescription":{
                "@":"root"
            },
            "_name":"form",
            "_valueType":"string",
            "helpString":"Specifies one or more forms the button belongs to"
        }
    },

    "button_description_formaction":{
        "prototype":"ui/component-description[ComponentPropertyDescription]",
        "properties":{
            "_componentDescription":{
                "@":"root"
            },
            "_name":"formaction",
            "_valueType":"url",
            "helpString":"Specifies where to send the form-data when a form is submitted. Only for type='submit'"
        }
    },

    "button_description_formenctype":{
        "prototype":"ui/component-description[ComponentPropertyDescription]",
        "properties":{
            "_componentDescription":{
                "@":"root"
            },
            "_name":"formenctype",
            "_valueType":"enum",
            "_enumValues":[
                "application/x-www-form-urlencoded",
                "multipart/form-data",
                "text/plain"],
            "helpString":"Specifies how form-data should be encoded before sending it to a server. Only for type='submit'"
        }
    },

    "button_description_formmethod":{
        "prototype":"ui/component-description[ComponentPropertyDescription]",
        "properties":{
            "_componentDescription":{
                "@":"root"
            },
            "_name":"formmethod",
            "_valueType":"enum",
            "_enumValues":[
                "get",
                "post"],
            "helpString":"Specifies how to send the form-data (which HTTP method to use). Only for type='submit'"
        }
    },

    "button_description_formnovalidate":{
        "prototype":"ui/component-description[ComponentPropertyDescription]",
        "properties":{
            "_componentDescription":{
                "@":"root"
            },
            "_name":"formnovalidate",
            "_valueType":"boolean",
            "helpString":"Specifies that the form-data should not be validated on submission. Only for type='submit'"
        }
    },

    "button_description_formtarget":{
        "prototype":"ui/component-description[ComponentPropertyDescription]",
        "properties":{
            "_componentDescription":{
                "@":"root"
            },
            "_name":"formtarget",
            "_valueType":"string",
            "helpString":"Specifies where to display the response after submitting the form. Only for type='submit'"
        }
    },

    "button_description_name":{
        "prototype":"ui/component-description[ComponentPropertyDescription]",
        "properties":{
            "_componentDescription":{
                "@":"root"
            },
            "_name":"name",
            "_valueType":"string",
            "helpString":"Specifies a name for the button"
        }
    },

    "button_description_label":{
        "prototype":"ui/component-description[ComponentPropertyDescription]",
        "properties":{
            "_componentDescription":{
                "@":"root"
            },
            "_name":"label",
            "_valueType":"string",
            "helpString":""
        }
    },

    "button_description_type":{
        "prototype":"ui/component-description[ComponentPropertyDescription]",
        "properties":{
            "_componentDescription":{
                "@":"root"
            },
            "_name":"type",
            "_valueType":"enum",
            "_enumValues":[
                "button",
                "reset",
                "submit"],
            "helpString":"Specifies the type of button"
        }
    },

    "button_description_value":{
        "prototype":"ui/component-description[ComponentPropertyDescription]",
        "properties":{
            "_componentDescription":{
                "@":"root"
            },
            "_name":"value",
            "_valueType":"string",
            "helpString":"Specifies an initial value for the button"
        }
    },

    "root":{
        "prototype":"ui/component-description",
        "properties":{
            "_componentPropertyDescriptions":{
                "autofocus":{
                    "@":"button_description_autofocus"
                },
                "enabled":{
                    "@":"button_description_enabled"
                },
                "form":{
                    "@":"button_description_form"
                },
                "formaction":{
                    "@":"button_description_formaction"
                },
                "formenctype":{
                    "@":"button_description_formenctype"
                },
                "formmethod":{
                    "@":"button_description_formmethod"
                },
                "formnovalidate":{
                    "@":"button_description_formnovalidate"
                },
                "formtarget":{
                    "@":"button_description_formtarget"
                },
                "name":{
                    "@":"button_description_name"
                },
                "label":{
                    "@":"button_description_label"
                },
                "type":{
                    "@":"button_description_type"
                },
                "value":{
                    "@":"button_description_value"
                }
            },
            "_componentPropertyDescriptionGroups":{
                "base":[
                    {
                        "@":"button_description_label"
                    },
                    {
                        "@":"button_description_type"
                    },
                    {
                        "@":"button_description_name"
                    },
                    {
                        "@":"button_description_enabled"
                    },
                    {
                        "@":"button_description_autofocus"
                    }
                ],
                "form":[
                    {
                        "@":"button_description_form"
                    },
                    {
                        "@":"button_description_formaction"
                    },
                    {
                        "@":"button_description_formenctype"
                    },
                    {
                        "@":"button_description_formmethod"
                    },
                    {
                        "@":"button_description_formnovalidate"
                    },
                    {
                        "@":"button_description_formtarget"
                    }
                ]
            },
            "_componentPropertyValidationRules":{
            }
        }
    }
}
